{
    "$schema": "https://docs.renovatebot.com/renovate-schema.json",
    "extends": ["config:recommended"],
    "dependencyDashboard": false,
    "addLabels": ["dependencies"],
    "assignees": ["hudsonm62"],
    "packageRules": [
        {
            "matchManagers": ["npm"],
            "matchDepTypes": ["devDependencies"],
            "enabled": false
        },
        {
            "matchManagers": ["gomod"],
            "matchDepTypes": ["indirect"],
            "enabled": true
        }
    ]
}
